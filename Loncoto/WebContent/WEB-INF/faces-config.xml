<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
<application>
        <el-resolver>
                org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
    <navigation-rule>
    	<from-view-id>index.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>liste</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
   		<navigation-case>
   			<from-outcome>editIntervention</from-outcome>
   			<to-view-id>editIntervention.xhtml</to-view-id>
   		</navigation-case> 	
   		<navigation-case>
   			<from-outcome>editIntervenant</from-outcome>
   			<to-view-id>editDocument.xhtml</to-view-id>
   		</navigation-case>
   		<navigation-case>
   			<from-outcome>editGroupe</from-outcome>
   			<to-view-id>editGroupe.xhtml</to-view-id>
   		</navigation-case>
   		<navigation-case>
   			<from-outcome>editMateriel</from-outcome>
   			<to-view-id>editMateriel.xhtml</to-view-id>
   		</navigation-case> 	
   		<navigation-case>
   			<from-outcome>editArticle</from-outcome>
   			<to-view-id>editArticle.xhtml</to-view-id>
   		</navigation-case>
   		<navigation-case>
   			<from-outcome>editSousFamille</from-outcome>
   			<to-view-id>editSousFamille.xhtml</to-view-id>
   		</navigation-case>
   		<navigation-case>
   			<from-outcome>editFamille</from-outcome>
   			<to-view-id>editFamille.xhtml</to-view-id>
   		</navigation-case>
   		<navigation-case>
   			<from-outcome>editClient</from-outcome>
   			<to-view-id>editClient.xhtml</to-view-id>
   		</navigation-case>
   		<navigation-case>
   			<from-outcome>editSite</from-outcome>
   			<to-view-id>editSite.xhtml</to-view-id>
   		</navigation-case>
   		<navigation-case>
   			<from-outcome>editBatiment</from-outcome>
   			<to-view-id>editBatiment.xhtml</to-view-id>
   		</navigation-case>
   		<navigation-case>
   			<from-outcome>editEtage</from-outcome>
   			<to-view-id>editEtage.xhtml</to-view-id>
   		</navigation-case>
   		<navigation-case>
   			<from-outcome>editSalle</from-outcome>
   			<to-view-id>editSalle.xhtml</to-view-id>
   		</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editIntervention.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml?faces-redirect=true</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editIntervenant.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editGroupe.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editMateriel.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editArticle.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editSousFamille.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editFamille.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editClient.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editSite.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editBatiment.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editEtage.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>editSalle.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>save</from-outcome>
    		<to-view-id>index.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>

</faces-config>
